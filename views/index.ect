<html>
  <head>
    <title>SSL gClient</title>
    <link href="/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="css/gclient.css" rel="stylesheet">
  </head>
  <body>
    <div class="container">
      <div class="row">
        <div class="col-md-10 col-md-offset-1 text-center">
          <span id="stage" class="label label-default state-name"></span>
          <span id="command" class="label label-default state-name"></span>
        </div>
      </div>
      <div class="row">
        <div class="col-md-10 col-md-offset-1 text-center">
          <div class="col-md-4">
            <span id="blue" class="label label-blue team-name"></span>
          </div>
          <div class="col-md-4">
            <span id="blue_score" class="label label-blue score-name"></span>
            <span id="center_bar" class="label label-default score-name">-</span>
            <span id="yellow_score" class="label label-yellow score-name"></span>
          </div>
          <div class="col-md-4">
            <span id="yellow" class="label label-yellow team-name"></span>
          </div>
        </div>
      </div>
      <div class="row">
        <!-- <canvas id="field" width="600" height="400"></canvas> -->
        <canvas class="col-md-10 col-md-offset-1" id="field" width="1125" height="750"></canvas>
      </div>
    </div>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/jquery/dist/jquery.min.js"></script>
    <script src="/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="/js/build.js"></script>
  </body>
</html>
